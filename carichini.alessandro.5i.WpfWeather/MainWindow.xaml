<Window x:Class="carichini.alessandro._5i.WpfWeather.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:carichini.alessandro._5i.WpfWeather"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="120"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="btn_Visualizza" Grid.Row="0" Click="Btn_Visualizza_Click" FontSize="40" FontWeight="Bold" Background="LightBlue">Visualizza!</Button>
        <ListView Grid.Row="1" x:Name="lsw_Dati"></ListView>
    </Grid>
</Window>
